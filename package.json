{
  "name": "cybernaut",
  "version": "0.0.0-development",
  "description": "Reliable, zero configuration end-to-end testing in BDD-style.",
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "keywords": [
    "bdd",
    "e2e",
    "end-to-end",
    "selenium",
    "test",
    "testing",
    "webdriver"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/cybernaut.git"
  },
  "bin": "dist/index.js",
  "main": "dist/index",
  "types": "dist/index",
  "engines": {
    "node": ">=6"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "ava": {
    "files": [
      "dist/**/*.test.js"
    ],
    "serial": true,
    "verbose": true
  },
  "nyc": {
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "all": true,
    "cache": false,
    "check-coverage": true,
    "exclude": [
      "_book/**/*.js",
      "coverage/**/*.js",
      "dist/**/*.test.js",
      "dist/index.js",
      "docker/**/*.js",
      "example/**/*.js",
      "scripts/**/*.js",
      "wallaby.js"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "scripts": {
    "prebuild": "npm run compile",
    "build": "./scripts/build.sh",
    "check": "./scripts/check.sh",
    "compile": "./scripts/compile.sh",
    "cz": "git-cz",
    "docs": "gitbook serve",
    "preexamples": "npm run build",
    "examples": "./scripts/examples.sh",
    "format": "tsfmt --replace",
    "pretest": "npm run compile",
    "test": "./scripts/test.sh",
    "prewatch": "npm run compile",
    "watch": "run-p --continue-on-error 'watch:*'",
    "watch:ava": "ava --fail-fast --watch",
    "watch:tsc": "tsc --project . --watch"
  },
  "dependencies": {
    "@types/fs-extra": "0.0.37",
    "@types/mz": "0.0.31",
    "@types/node": "7.0.14",
    "@types/proxyquire": "1.3.27",
    "ajv": "5.0.0",
    "chromedriver": "2.29.0",
    "debug": "2.6.4",
    "deep-strict-equal": "0.2.0",
    "fs-promise": "2.0.2",
    "geckodriver": "1.6.0",
    "glob": "7.1.1",
    "selenium-webdriver": "3.4.0",
    "tap": "10.3.2",
    "tslib": "1.6.1",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "commitizen": "2.9.6",
    "conventional-changelog-lint": "1.1.9",
    "coveralls": "2.13.0",
    "custom-tslint-formatters": "2.1.0",
    "cz-customizable": "5.0.0",
    "gitbook-cli": "2.3.0",
    "gitbook-plugin-anchorjs": "1.1.1",
    "gitbook-plugin-edit-link": "2.0.2",
    "gitbook-plugin-favicon": "0.0.2",
    "gitbook-plugin-github": "3.0.0",
    "markdownlint": "0.4.1",
    "npm-run-all": "4.0.2",
    "nyc": "10.2.0",
    "proxyquire": "1.7.11",
    "semantic-release": "6.3.2",
    "sinon": "2.1.0",
    "ts-config": "5.0.0",
    "tslint": "5.1.0",
    "typescript": "2.2.2",
    "typescript-formatter": "5.1.3"
  }
}
