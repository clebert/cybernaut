{
  "name": "cybernaut",
  "version": "6.1.0",
  "description": "Reliable, zero configuration end-to-end testing in BDD-style.",
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "keywords": [
    "bdd",
    "e2e",
    "end-to-end",
    "selenium",
    "test",
    "testing",
    "webdriver"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/cybernaut.git"
  },
  "bin": "dist/index.js",
  "main": "dist/index",
  "types": "dist/index",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "_book/",
      "node_modules/"
    ],
    "testRegex": "\\.test\\.ts$",
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "scripts": {
    "prebuild": "npm run compile",
    "build": "./scripts/build.sh",
    "check": "./scripts/check.sh",
    "compile": "./scripts/compile.sh",
    "cz": "git-cz",
    "predocs": "gitbook install",
    "docs": "gitbook serve",
    "preexamples": "npm run build",
    "examples": "./scripts/examples.sh",
    "format": "tsfmt --replace",
    "prepush": "npm run examples",
    "push": "./scripts/push.sh",
    "release": "standard-version",
    "test": "./scripts/test.sh",
    "watch": "jest --verbose --watch"
  },
  "dependencies": {
    "ajv": "5.1.2",
    "chromedriver": "2.29.0",
    "debug": "2.6.8",
    "deep-strict-equal": "0.2.0",
    "geckodriver": "1.6.1",
    "globby": "6.1.0",
    "selenium-webdriver": "3.4.0",
    "tap": "10.3.2",
    "tslib": "1.7.1"
  },
  "devDependencies": {
    "@types/jest": "19.2.3",
    "@types/node": "7.0.19",
    "commitizen": "2.9.6",
    "conventional-changelog-lint": "1.1.9",
    "coveralls": "2.13.1",
    "custom-tslint-formatters": "2.1.0",
    "cz-customizable": "5.0.0",
    "git-latest-semver-tag": "1.0.2",
    "gitbook-cli": "2.3.0",
    "jest": "20.0.3",
    "markdownlint": "0.4.1",
    "standard-version": "4.0.0",
    "ts-config": "7.1.0",
    "ts-jest": "20.0.4",
    "tslint": "5.2.0",
    "typescript": "2.3.2",
    "typescript-formatter": "5.2.0"
  }
}
