{
  "name": "cybernaut",
  "version": "0.0.0",
  "description": "TODO",
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/cybernaut.git"
  },
  "bin": "./dist/index.js",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=6"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "ava": {
    "files": [
      "dist/**/*.test.js"
    ],
    "serial": true,
    "verbose": true
  },
  "nyc": {
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "all": false,
    "cache": false,
    "check-coverage": true,
    "exclude": [
      "dist/**/__mocks__/**",
      "dist/**/__tests__/**"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "scripts": {
    "commitmsg": "conventional-changelog-lint -e",
    "prebuild": "rm -rf dist/*",
    "build": "tsc --project .",
    "postbuild": "chmod +x dist/index.js",
    "check": "tsfmt --verify && tslint --config tslint.json --project tsconfig.json --type-check --formatters-dir node_modules/custom-tslint-formatters/formatters --format grouped",
    "cz": "git-cz",
    "format": "tsfmt --replace",
    "prerelease": "git checkout master && npm prune && npm install && npm test",
    "release": "standard-version",
    "pretest": "npm run build",
    "test": "npm run check && nyc ava",
    "prestart": "npm run build",
    "start": "./dist/index.js | tap-mocha-reporter spec",
    "prewatch": "npm run build",
    "watch": "run-p --continue-on-error watch:*",
    "watch:sources": "tsc --project . --watch",
    "watch:tests": "ava --fail-fast --watch"
  },
  "dependencies": {
    "ajv": "4.11.5",
    "deep-strict-equal": "0.2.0",
    "glob": "7.1.1",
    "selenium-webdriver": "3.3.0",
    "tap": "10.3.0",
    "tslib": "1.6.0"
  },
  "devDependencies": {
    "@types/node": "7.0.10",
    "@types/proxyquire": "1.3.27",
    "ava": "0.18.2",
    "chromedriver": "2.28.0",
    "commitizen": "2.9.6",
    "conventional-changelog-lint": "1.1.7",
    "custom-tslint-formatters": "2.0.0",
    "cz-customizable": "4.0.0",
    "husky": "0.13.2",
    "npm-run-all": "4.0.2",
    "nyc": "10.1.2",
    "proxyquire": "1.7.11",
    "sinon": "2.1.0",
    "standard-version": "4.0.0",
    "tap-mocha-reporter": "3.0.3",
    "ts-config": "3.1.0",
    "tslint": "4.5.1",
    "typescript": "2.2.1",
    "typescript-formatter": "5.1.2"
  }
}
