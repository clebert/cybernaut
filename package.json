{
  "name": "cybernaut",
  "version": "0.0.0-development",
  "description": "Reliable, zero configuration end-to-end testing in BDD-style.",
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/cybernaut.git"
  },
  "bin": "dist/index.js",
  "main": "dist/index",
  "typings": "dist/index",
  "engines": {
    "node": ">=6"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "ava": {
    "files": [
      "dist/**/*.test.js"
    ],
    "serial": true,
    "verbose": true
  },
  "lint-staged": {
    "*.ts": [
      "tsfmt --replace",
      "git add"
    ]
  },
  "nyc": {
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "all": true,
    "cache": false,
    "check-coverage": true,
    "exclude": [
      "coverage/**",
      "dist/**/__tests__/**",
      "dist/config.js",
      "dist/index.js",
      "example/**",
      "fixtures/**",
      "wallaby.js"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "scripts": {
    "commitmsg": "conventional-changelog-lint --edit",
    "precommit": "lint-staged",
    "prebuild": "rm -rf dist/*",
    "build": "tsc --project .",
    "postbuild": "chmod +x dist/index.js",
    "check": "tsfmt --verify && tslint --config tslint.json --project tsconfig.json --type-check --formatters-dir node_modules/custom-tslint-formatters/formatters --format grouped \"{src,typings}/**/*.ts\"",
    "cz": "git-cz",
    "format": "tsfmt --replace",
    "prerelease": "npm run build",
    "release": "semantic-release pre && npm publish && semantic-release post",
    "pretest": "npm run build",
    "test": "conventional-changelog-lint --from=HEAD~1 && npm run check && nyc ava",
    "prewatch": "npm run build",
    "watch": "run-p --continue-on-error watch:*",
    "watch:sources": "tsc --project . --watch",
    "watch:tests": "ava --fail-fast --watch"
  },
  "dependencies": {
    "@types/node": "7.0.12",
    "@types/proxyquire": "1.3.27",
    "ajv": "4.11.5",
    "deep-strict-equal": "0.2.0",
    "glob": "7.1.1",
    "selenium-webdriver": "3.3.0",
    "tap": "10.3.1",
    "tslib": "1.6.0"
  },
  "devDependencies": {
    "ava": "0.18.2",
    "commitizen": "2.9.6",
    "conventional-changelog-lint": "1.1.7",
    "coveralls": "2.13.0",
    "custom-tslint-formatters": "2.1.0",
    "cz-customizable": "4.0.0",
    "husky": "0.13.3",
    "lint-staged": "3.4.0",
    "npm-run-all": "4.0.2",
    "nyc": "10.2.0",
    "proxyquire": "1.7.11",
    "semantic-release": "6.3.2",
    "sinon": "2.1.0",
    "ts-config": "3.2.0",
    "tslint": "4.5.1",
    "typescript": "2.2.2",
    "typescript-formatter": "5.1.2"
  }
}
